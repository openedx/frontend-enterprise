{
  "name": "@edx/frontend-enterprise-catalog-search",
  "version": "2.8.0",
  "description": "Components related to Enterprise catalog search.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-enterprise.git"
  },
  "browserslist": [
    "extends @edx/browserslist-config"
  ],
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx"
    }
  },
  "scripts": {
    "dev": "npm-watch build",
    "clean": "make clean",
    "build": "make build",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "lint:fix": "fedx-scripts eslint --fix --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage --passWithNoTests"
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/edx/frontend-enterprise#readme",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "bugs": {
    "url": "https://github.com/edx/frontend-enterprise/issues"
  },
  "sideEffects": false,
  "dependencies": {
    "@edx/frontend-enterprise-utils": "^1.3.0",
    "classnames": "2.2.5",
    "lodash.debounce": "4.0.8",
    "prop-types": "15.6.2"
  },
  "devDependencies": {
    "@edx/browserslist-config": "1.0.0",
    "@edx/frontend-build": "5.6.14",
    "@fortawesome/free-solid-svg-icons": "5.8.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.6",
    "@testing-library/react-hooks": "3.4.2",
    "@testing-library/user-event": "13.1.8",
    "npm-watch": "^0.9.0",
    "react-instantsearch-dom": "6.8.3"
  },
  "peerDependencies": {
    "@edx/frontend-platform": "1.9.6",
    "@edx/paragon": "^19.15.0",
    "@fortawesome/free-solid-svg-icons": "5.8.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-instantsearch-dom": "6.8.3",
    "react-router-dom": "5.2.0"
  }
}
